{
  "compilerOptions": {
    "target": "ES2016",
    "rootDir": "./src/",
    "moduleResolution": "Node",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "outDir": "./dist/",
    "strict": true,
    "skipLibCheck": true
  }
}
